{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "extract"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "styles",
        "META-INF",
        "WEB-INF",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "assets/styles/core.scss",
        "assets/styles/vendor.scss",
        "assets/styles/main.scss",
        "styles.scss",
        "../node_modules/primeng/resources/primeng.min.css",
        "assets/themes/bootstrap/theme.scss"
      ],
      "scripts": [
        "assets/scripts/core.js",
        "assets/scripts/shiv.js",
        "assets/scripts/ace.js",
        "assets/scripts/pikadaytime.js",
        "assets/scripts/handsontable.full.min.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "component": {
      "viewEncapsulation": "None"
    }
  }
}
